roup>\n          <Form.Group>\n            <InputGroup>\n            <Form.Control\n              className=\"login-field\"\n              placeholder=\"Password\"\n              required\n              type=\"password\"\n              onChange={(event) => {\n                setPassword(event.target.value);\n              }}\n            />\n            </InputGroup>\n            <FormControl.Feedback type=\"invalid\">\n              Please enter password\n            </FormControl.Feedback>\n          </Form.Group>\n          <Button variant=\"primary\" type=\"submit\" className=\"login-login-btn\">\n            SIGN IN\n          </Button>\n          <div className=\"login-forgot-password\">\n            <Link to=\"/forgotpassword\"> FORGOT PASSWORD </Link>\n          </div>\n        </Form>\n      </div>\n    </div>\n  );\n};\n\nexport default Login;\n","/Users/trexdex/Documents/Projects/codingkidz/quizapp/src/pages/Units/UnitsPage.tsx",[],"/Users/trexdex/Documents/Projects/codingkidz/quizapp/src/services/firebase.ts",[],"/Users/trexdex/Documents/Projects/codingkidz/quizapp/src/components/PublicRoute.tsx",[],"/Users/trexdex/Documents/Projects/codingkidz/quizapp/src/components/PrivateRoute.tsx",[],"/Users/trexdex/Documents/Projects/codingkidz/quizapp/src/pages/Units/UnitContainer.tsx",[],"/Users/trexdex/Documents/Projects/codingkidz/quizapp/src/pages/EditTopic/EditQuestionForm.tsx",[],"/Users/trexdex/Documents/Projects/codingkidz/quizapp/src/pages/Quiz/LivesContainer.tsx",[],"/Users/trexdex/Documents/Projects/codingkidz/quizapp/src/pages/Quiz/QuestionContainer.tsx",[],"/Users/trexdex/Documents/Projects/codingkidz/quizapp/src/pages/Units/TopicContainer.tsx",[],"/Users/trexdex/Documents/Projects/codingkidz/quizapp/src/pages/EditTopic/TrueFalseForm.tsx",[],"/Users/trexdex/Documents/Projects/codingkidz/quizapp/src/pages/EditTopic/MultipleChoiceForm.tsx",[],"/Users/trexdex/Documents/Projects/codingkidz/quizapp/src/models/index.tsx",[],["113","114"],"/Users/trexdex/Docu